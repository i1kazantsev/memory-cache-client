{
  "name": "@i1k/memory-cache-client",
  "version": "0.0.11",
  "author": "Ivan Kazantsev",
  "license": "MIT",
  "description": "Memory cache client with the same interface as @ik/smart-cache-manager.",
  "homepage": "https://github.com/i1kazantsev/memory-cache-client#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/i1kazantsev/memory-cache-client"
  },
  "bugs": {
    "url": "https://github.com/i1kazantsev/memory-cache-client/issues"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc",
    "test": "jest --coverage",
    "lint:js": "eslint --ext .js,.ts --ignore-path .gitignore .",
    "lint:prettier": "prettier --check .",
    "lint": "pnpm lint:js && pnpm lint:prettier",
    "lintfix": "prettier --write --list-different . && pnpm lint:js -- --fix",
    "release": "pnpm lint && pnpm test && pnpm build && standard-version",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --cache",
    "*.**": "prettier --check --ignore-unknown"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.5.0",
    "@commitlint/config-conventional": "^17.4.4",
    "@i1k/smart-cache-manager": "0.0.14",
    "@types/jest": "^29.5.0",
    "@types/micromatch": "^4.0.2",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.0",
    "prettier": "^2.8.7",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "glob": "^10.2.5",
    "lru-cache": "^9.1.1",
    "micromatch": "^4.0.5"
  },
  "keywords": [
    "cache client",
    "memory"
  ]
}
